# OpenSSL configuration file.

[req]
default_md  = sha256
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no

[req_distinguished_name]
C = CY
ST = Nicosia
L = Nicosia
O = High Level Software Ltd
CN = ca.localdomain.com

[v3_req]
keyUsage = critical, digitalSignature, keyAgreement, keyEncipherment, dataEncipherment, keyCertSign, cRLSign
basicConstraints = CA:FALSE
extendedKeyUsage = serverAuth, clientAuth